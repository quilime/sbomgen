{
    "compilerOptions": {
      "lib": ["es2020"],
      "module": "commonjs",
      "target": "es6",
      "moduleResolution": "node",
      "rootDir" : "./src",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude":[
        "./node_modules"
    ]
  }